{
  "name": "ubergreen.org",
  "version": 2,
  "alias": ["www.ubergreen.org"],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "status": 301,
      "headers": { "Location": "https://www.ubergreen.org/$1" }
    }
  ]
}

{
  "name": "ubergreen.org",
  "version": "2.0.1",
  "description": "Ubergreen site",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "eslint": "eslint src/ --ext .js,.jsx",
    "stylelint": "stylelint 'src/**/*.css' --config ./stylelint.config.js",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run eslint && npm run stylelint"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callmenick/ubergreen.org.git"
  },
  "author": "Nick Salloum <nick@callmenick.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/callmenick/ubergreen.org/issues"
  },
  "homepage": "https://github.com/callmenick/ubergreen.org#readme",
  "dependencies": {
    "@zeit/next-css": "1.0.1",
    "classnames": "2.2.6",
    "cookie-parser": "1.4.4",
    "cross-env": "5.2.0",
    "express": "4.18.2",
    "lodash": "4.17.15",
    "next": "9.1.1",
    "next-images": "1.2.0",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "sitemap": "2.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "babel-eslint": "10.0.2",
    "cssnano": "4.1.10",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-cypress": "2.2.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "husky": "2.7.0",
    "lint-staged": "8.2.1",
    "postcss-preset-env": "6.6.0",
    "prettier": "1.18.2",
    "stylelint": "10.1.0"
  },
  "engines": {
    "node": "^10.15",
    "npm": "^6.5"
  },
  "private": true
}
